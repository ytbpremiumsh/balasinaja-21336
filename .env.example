# Server Configuration
PORT=3000

# OneSender API Configuration
ONESENDER_API_URL=https://api.onesender.id/api/v1/message/send
ONESENDER_API_KEY=your_onesender_api_key_here

# Message Priority (1-10)
MESSAGE_PRIORITY=10

# AI Configuration
# Supported vendors: gemini, openai, deepseek, openrouter
AI_VENDOR=gemini
AI_API_KEY=your_ai_api_key_here
AI_MODEL=gemini-pro

# For OpenAI: gpt-4, gpt-3.5-turbo
# For Gemini: gemini-pro, gemini-1.5-pro
# For DeepSeek: deepseek-chat
# For OpenRouter: openrouter/auto atau model spesifik
